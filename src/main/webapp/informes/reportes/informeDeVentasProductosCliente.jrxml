	</field>
	<field name="cfg_producto_idProducto" class="java.lang.Long">
		<fieldDescription><![CDATA[]]></fieldDescription>
	</field>
	<field name="max(f.fecCrea)" class="java.sql.Timestamp"/>
	<field name="producto" class="java.lang.String"/>
